<h1>Users#new</h1>
<p>Find me in app/views/users/new.html.erb</p>

<%= form_for User.new do |f| %>
	<%= f.label :fname %>
	<%= f.text_field :fname %><br>
	<%= f.label :lname %>
	<%= f.text_field :lname %><br>
	<%= f.label :email %>
	<%= f.text_field :email %><br>
	<%= f.label :password %>
	<%= f.text_field :password %><br>
	<%= f.submit %>
<%= yield %>
<% end %>